{
  "compilerOptions": {
    "outDir": "./dist",
    "noEmit": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "target": "es2016",
    "module": "commonjs",
    "skipLibCheck": true
  },
  "include": ["./src"],
  "exclude": ["node_modules", "dist", ".storybook"]
}
